{
  "name": "podcasthub.js",
  "version": "0.0.1",
  "description": "Podcast Manager",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "lintfix": "./node_modules/.bin/eslint . --fix",
    "pretest": "npm run lint ",
    "pretest_old": "npm run lint && NODE_ENV=test node_modules/.bin/sequelize db:migrate",
    "test": "NODE_ENV=test mocha -r ts-node/register 'test/**/*.ts'",
    "test-single": "NODE_ENV=test mocha -r ts-node/register 'test/parsing-xml-to-json*.ts'",
    "testsingle": "NODE_ENV=test mocha test/podcast-with-episode-read.test.js"
  },
  "bin": {
    "pch-add-podcast": "./add-podcast.js",
    "pch-add-subscription": "./add-subscription.js",
    "pch-cleanup": "./cleanup.js",
    "pch-destroy-podcast": "./destroy-podcast.js",
    "pch-export-opml": "./export-opml.js",
    "pch-import-opml": "./import-opml.js",
    "pch-list-podcasts": "./list-podcasts.js",
    "pch-show-podcast": "./show-podcast.js",
    "pch-update-podcasts": "./update-podcasts.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaredbangs/PodcastHub.js.git"
  },
  "author": "Jared Bangs",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/jaredbangs/PodcastHub.js/issues"
  },
  "homepage": "https://github.com/jaredbangs/PodcastHub.js#readme",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/chai": "^5.0.0",
    "@types/chai-datetime": "^1.0.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.7.5",
    "chai": "^5.1.1",
    "chai-datetime": "^1.8.1",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "mocha": "^10.7.3",
    "timezone-mock": "^1.0.2",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.8.1"
  },
  "dependencies": {
    "@types/pouchdb": "^6.4.2",
    "@types/pouchdb-find": "^7.3.3",
    "bluebird": "^3.5.2",
    "dotenv": "^8.0.0",
    "moment": "^2.22.2",
    "node-opml-parser": "^1.0.0",
    "node-podcast-parser": "^2.3.0",
    "opml-generator": "^1.1.1",
    "pouchdb": "^9.0.0",
    "pouchdb-find": "^9.0.0",
    "typescript": "^5.5.4",
    "winston": "^3.1.0",
    "xml-formatter": "^1.1.0"
  }
}
